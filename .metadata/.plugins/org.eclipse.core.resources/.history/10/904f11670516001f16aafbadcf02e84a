package zadatal1;

public class ProbaKonvertoraTemperature {
 
	public static void main(String[] args) {
	 KonvertorTemperature kt= new KonvertorTemperature();
	System.out.println(kt.konvertujUCelzijuse(-273.15));
	System.out.println(kt.konvertujUFarenhajte(-8273.15));
	 System.out.println(kt.vratiAgregatnoStanjeVode(124));

	}
}
